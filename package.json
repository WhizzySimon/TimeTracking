{
	"name": "timetracker",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "node scripts/build/update-version.js && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo '' && node scripts/setup/setup-hooks.js",
		"setup-hooks": "node scripts/setup/setup-hooks.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:ci": "playwright test --reporter=line,json --output=playwright-results.json",
		"test:webkit": "playwright test --project=webkit",
		"test:ios": "playwright test --project=\"Mobile Safari\"",
		"test:webkit:ui": "playwright test --ui --project=webkit",
		"test:ios:ui": "playwright test --ui --project=\"Mobile Safari\"",
		"test": "npm run test:unit && npm run test:e2e",
		"check:pwa": "node scripts/build/check-pwa.js",
		"verify": "powershell -ExecutionPolicy Bypass -File scripts/build/verify-code.ps1",
		"wb": "tsx devBrowser.ts",
		"ai:log-event": "node scripts/ai/telemetry.cjs log-event",
		"ai:start-command": "node scripts/ai/telemetry.cjs start-command",
		"ai:end-command": "node scripts/ai/telemetry.cjs end-command",
		"ai:fingerprint-error": "node scripts/ai/telemetry.cjs fingerprint-error",
		"ai:detect-anomalies": "node scripts/ai/anomaly-detector.cjs",
		"ai:evidence": "node scripts/ai/evidence-generator.cjs",
		"ai:extract-learnings": "node scripts/ai/learning-extractor.cjs"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^22",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"playwright": "^1.57.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.89.0",
		"jspdf": "^3.0.4",
		"jspdf-autotable": "^5.0.2",
		"xlsx": "^0.18.5"
	}
}
